package if_ex01;

public class dasdadad {

	public static void main(String[] args) {
		int score = 80 ;
		
		if(score>=90) {
			System.out.println("점수는 90보다 큽니다.");
			System.out.println("등급은 A등급입니다.");
		}
		
		else(score<90) 
			System.out.println("점수는 90보다 작습니다.");
			System.out.println("등급은 B등급 입니다.");
		
	}
	
}
